<div class="resume-container">
    <div class="heading-container">
        <h1>Resume Builder</h1>
    </div>
    <div class="resume-form-container">
        @{
            var a = (FirstWebsite.Models.Resume) ViewData["resume"];

            <h1>Name: @a.Name</h1>
            <h1>Age: @a.Age</h1>
            <h1>Email: @a.Email</h1>
        }
        <ul>
            @{
                foreach(string i in (List<string>) ViewData["skills"])
                {
                    <li>@i</li>
                }
            }
        </ul>

        <br /><br /><br />
        <h1>Employee</h1>
        <h3>@ViewBag.Employee.Name</h3>
        <h3>@ViewBag.Employee.Age</h3>
        <h3>@ViewBag.Employee.Email</h3>

        <p>@ViewBag.Salary</p>
    </div>
</div>